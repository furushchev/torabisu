{
    "name": "Travis Log Colorizer",
    "version": "1.0",
    "description": "For everyone who want to grow your own QOL",
    "manifest_version": 2,
    "browser_action": {
	"default_icon": "icon.png"
    },
    "content_scripts": [
	{
	    "matches": ["https://s3.amazonaws.com/archive.travis-ci.org/jobs/*/log.txt"],
	    "js": ["jquery.min.js", "ansi_up.js", "colorize.js"]
	}
    ]
}
